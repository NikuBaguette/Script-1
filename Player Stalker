-- Scripts:

local function HQFML_fake_script() -- StandViewer.Script 
	local script = Instance.new('LocalScript', StandViewer)

	local boutton = script.Parent.Frame.Activation
	local text = script.Parent.Frame.Pseudo
	local Cringe = script.Parent.Frame.Cringe
	local Players = game:GetService("Players")
	local Pinput = game:GetService("UserInputService")
	local frame = script.Parent.Frame
	local Find = script.Parent.Finded
	local item = script.Parent.item
	
	frame.close.MouseButton1Click:Connect(function()
		script.Parent:Destroy()
	end)
	
	frame.Players.cclose.MouseButton1Click:Connect(function()
		frame.Players.Visible = false
	end)
	
	Pinput.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.RightControl then
			if frame.Visible == true then
				frame.Visible = false
			else
				frame.Visible = true
			end
			if Find.Visible == true then
				Find.Visible = false
				item.Visible = false
			end
		end
	end)
	
	Find.close.MouseButton1Click:Connect(function()
		Find.Visible = false
		item.Visible = false
	end)
	frame.Cringe.MouseButton1Click:Connect(function()
		if item.Visible == false then
			item.Visible = true
		else
			item.Visible = false
		end
	end)
	
	Find.What.MouseButton1Click:Connect(function()
		if item.Visible == false then
			item.Visible = true
		else
			item.Visible = false
		end
	end)
	
	boutton.MouseButton1Click:Connect(function()
		Find.Visible = true
		local plr = Players:FindFirstChild(text.Text)
		local data = Players[plr.Name]
		Find.Named.Text = "Name : "..text.Text
		Find.Stand.Text = "Stand : "..data.PlayerStats.Stand.Value
		Find.Spec.Text = "Spec : "..data.PlayerStats.Spec.Value
		Find.Level.Text = "Level : "..data.PlayerStats.Level.Value
		Find.Prestige.Text = "Prestige : "..data.PlayerStats.Prestige.Value
		Find.Money.Text = "Money : "..data.PlayerStats.Money.Value
		Find.Race.Text = "Race : "..data.PlayerStats.Race.Value
		Find.Gang.Text = "Gang : "..data.PlayerStats.Gang.Value
	end)
	
	item.Arrow.MouseButton1Click:Connect(function()
		local it = 0
		local inv = {}
		local kown = 0
		local rep = ""
		if Find.Visible == true then
			local plr = Players:FindFirstChild(text.Text)
			local data = plr
			for i, arrow in pairs(data.Backpack:GetChildren()) do
				if arrow.Name == "Mysterious Arrow" or arrow.Name == "Stand Arrow" then
					it = it +1
				end
			end
			Find.What.Text = "How many : "..it
		else
			for i, name in pairs(Players:GetPlayers()) do
				local data = name
				for i, arrow in pairs(data.Backpack:GetChildren()) do
					if arrow.Name == "Mysterious Arrow" or arrow.Name == "Stand Arrow" then
						kown = 1
					end
				end
				if kown == 1 then
					table.insert(inv, name)
				end
			end
			for k, v in pairs(inv) do
				rep = rep..v.Name.." "
			end
			frame.Players.Text = rep
			frame.Players.Visible = true
		end
	end)
	
	item.Roka.MouseButton1Click:Connect(function()
		local it = 0
		local inv = {}
		local kown = 0
		local rep = ""
		if Find.Visible == true then
			local plr = Players:FindFirstChild(text.Text)
			local data = Players[plr.Name]
			for i, arrow in pairs(data.Backpack:GetChildren()) do
				if arrow.Name == "Rokakaka"then
					it = it +1
				end
			end
			Find.What.Text = "How many : "..it
		else
			for i, name in pairs(Players:GetPlayers()) do
				local data = name
				for i, arrow in pairs(data.Backpack:GetChildren()) do
					if arrow.Name == "Rokakaka" then
						kown = 1
					end
				end
				if kown == 1 then
					table.insert(inv, name)
				end
			end
			for k, v in pairs(inv) do
				rep = rep..v.Name.." "
			end
			frame.Players.Text = rep
			frame.Players.Visible = true
		end
	end)
	
	item.Pure.MouseButton1Click:Connect(function()
		local it = 0
		local inv = {}
		local kown = 0
		local rep = ""
		if Find.Visible == true then
			local plr = Players:FindFirstChild(text.Text)
			local data = Players[plr.Name]
			for i, arrow in pairs(data.Backpack:GetChildren()) do
				if arrow.Name == "Pure Rokakaka"then
					it = it +1
				end
			end
			Find.What.Text = "How many : "..it
		else
			for i, name in pairs(Players:GetPlayers()) do
				local data = name
				for i, arrow in pairs(data.Backpack:GetChildren()) do
					if arrow.Name == "Pure Rokakaka" then
						kown = 1
					end
				end
				if kown == 1 then
					table.insert(inv, name)
				end
			end
			for k, v in pairs(inv) do
				rep = rep..v.Name.." "
			end
			frame.Players.Text = rep
			frame.Players.Visible = true
		end
	end)
	
	item.Rib.MouseButton1Click:Connect(function()
