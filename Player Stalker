			local data = Players[plr.Name]
			for i, arrow in pairs(data.Backpack:GetChildren()) do
				if arrow.Name == "Ancient Scroll"then
					it = it +1
				end
			end
			Find.What.Text = "How many : "..it
		else
			for i, name in pairs(Players:GetPlayers()) do
				local data = name
				for i, arrow in pairs(data.Backpack:GetChildren()) do
					if arrow.Name == "Ancient Scroll" then
						kown = 1
					end
				end
				if kown == 1 then
					table.insert(inv, name)
				end
			end
			for k, v in pairs(inv) do
				rep = rep..v.Name.." "
			end
			frame.Players.Text = rep
			frame.Players.Visible = true
		end
	end)
	
	item.Diamond.MouseButton1Click:Connect(function()
		local it = 0
		local inv = {}
		local kown = 0
		local rep = ""
		if Find.Visible == true then
			local plr = Players:FindFirstChild(text.Text)
			local data = Players[plr.Name]
			for i, arrow in pairs(data.Backpack:GetChildren()) do
				if arrow.Name == "Diamond"then
					it = it +1
				end
			end
			Find.What.Text = "How many : "..it
		else
			for i, name in pairs(Players:GetPlayers()) do
				local data = name
				for i, arrow in pairs(data.Backpack:GetChildren()) do
					if arrow.Name == "Diamond" then
						kown = 1
					end
				end
				if kown == 1 then
					table.insert(inv, name)
				end
			end
			for k, v in pairs(inv) do
				rep = rep..v.Name.." "
			end
			frame.Players.Text = rep
			frame.Players.Visible = true
		end
	end)
	
	item.Hamon.MouseButton1Click:Connect(function()
		local it = 0
		local inv = {}
		local kown = 0
		local rep = ""
		if Find.Visible == true then
			local plr = Players:FindFirstChild(text.Text)
			local data = Players[plr.Name]
			for i, arrow in pairs(data.Backpack:GetChildren()) do
				if arrow.Name == "Zepellin's Headband"then
					it = it +1
				end
			end
			Find.What.Text = "How many : "..it
		else
			for i, name in pairs(Players:GetPlayers()) do
				local data = name
				for i, arrow in pairs(data.Backpack:GetChildren()) do
					if arrow.Name == "Zepellin's Headband" then
						kown = 1
					end
				end
				if kown == 1 then
					table.insert(inv, name)
				end
			end
			for k, v in pairs(inv) do
				rep = rep..v.Name.." "
			end
			frame.Players.Text = rep
			frame.Players.Visible = true
		end
	end)
end
coroutine.wrap(HQFML_fake_script)()
